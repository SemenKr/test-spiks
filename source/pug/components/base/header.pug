mixin header(headerMods, currentUrl)
  -
    const headerLinks = [
      {
        href: "index.html",
        text: "Главная"
      },
      {
        href: "#",
        text: "Страница 2"
      },
      {
        href: "#",
        text: "Страница 3"
      },
    ]
  header.header
    .container
      a.logo.header__logo(href="index.html", aria-label="Переход на главную страницу") Marketplace
      +search-form()
      +custom-toggle({
        inputLabel: "Чекбокс",
        inputName: "user-agreement",
        inputValue: "user-agreement-1",
        inputChecked: false
      }).custom-toggle--checkbox
      +custom-toggle({
        inputLabel: "Concentrate",
        inputName: "user-agreement",
        inputValue: "user-agreement-1",
        inputChecked: false,
        inputIcon: "icon-leaf"
      }).custom-toggle--badge
